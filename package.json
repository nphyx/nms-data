{
  "name": "nms-data",
  "version": "0.0.1",
  "description": "Static datasets for No Man's Sky javascript apps.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/ test/",
    "flow": "flow check",
    "test": "npm run lint && nyc --reporter=text mocha --bail --require should --require babel-register --reporter=dot test/**/*",
